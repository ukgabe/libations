<div class="card">
  <div class="card-header d-flex align-items-center justify-content-between">
    Venue Likes

    <a data-toggle="collapse" href="#venue_venue_likes_new_form">
      Add venue like
    </a>
  </div>

  <ul class="list-group list-group-flush">
    <% @venue.venue_likes.each do |venue_like| %>
      <li class="list-group-item d-flex align-items-center justify-content-between">
        <a href="/venue_likes/<%= venue_like.id %>">
          <%= venue_like.user_id %>
        </a>

        <a href="/delete_venue_like_from_venue/<%= venue_like.id %>">
          Delete
        </a>

      </li>
    <% end %>

    <div class="card-footer">
      <div class="collapse" id="venue_venue_likes_new_form">
        <%= render partial: "venue_templates/venue_venue_likes_new_form" %>
      </div>
    </div>

  </ul>
</div>

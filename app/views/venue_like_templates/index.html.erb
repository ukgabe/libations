<div class="row mb-3">
  <div class="col-md-12">
    <h1>
      All venue likes
    </h1>

    <a href="/venue_likes/new" class="btn btn-block btn-outline-secondary">
      Add a new venue like
    </a>
  </div>
</div>

<!-- Ransack Code start -->

<!-- Ransack Code end -->

<!-- Location Map Code start -->

<!-- Location Map Code end -->

<hr>

<div class="row mb-3">
  <div class="col-md-12">
    <table class="table">
      <tr>
        <th>
          ID
        </th>

        <th>
          User
        </th>

        <th>
          Venue
        </th>

        <th>
          Created at
        </th>

        <th>
          Updated at
        </th>

        <th>
        </th>
      </tr>

      <% @venue_likes.each do |venue_like| %>
      <tr>
        <!-- Display venue_like.id start -->
        <td>
          <%= venue_like.id %>
        </td>
        <!-- Display venue_like.id end -->

        <!-- Display venue_like.user_id start -->
        <td>
          <%= venue_like.user_id %>
        </td>
        <!-- Display venue_like.user_id end -->

        <!-- Display venue_like.venue_id start -->
        <td>
          <%= venue_like.venue_id %>
        </td>
        <!-- Display venue_like.venue_id end -->

        <!-- Display venue_like.created_at start -->
        <td>
          <%= time_ago_in_words(venue_like.created_at) %> ago
        </td>
        <!-- Display venue_like.created_at end -->

        <!-- Display venue_like.updated_at start -->
        <td>
          <%= time_ago_in_words(venue_like.updated_at) %> ago
        </td>
        <!-- Display venue_like.updated_at end -->

        <td>
          <a href="/venue_likes/<%= venue_like.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>

<div class="row mb-3">
  <div class="col-md-8 offset-md-2">
    <h1>
      Venue like #<%= @venue_like.id %> details
    </h1>

    <div class="row mb-3">
      <div class="col">
        <a href="/venue_likes" class="btn btn-block btn-outline-secondary">
          Go back
        </a>
      </div>

      <div class="col">
        <a href="/venue_likes/<%= @venue_like.id %>/edit" class="btn btn-block btn-outline-secondary">
          Edit venue like
        </a>
      </div>

      <div class="col">
        <a href="/delete_venue_like/<%= @venue_like.id %>" class="btn btn-block btn-outline-secondary">
          Delete venue like
        </a>
      </div>
    </div>

    <dl>
      <dt>
        User
      </dt>
      <dd>
        <% if @venue_like.user.present? %>
          <a href="/users/<%= @venue_like.user_id %>">
            <%= @venue_like.user.created_at %>
          </a>
        <% end %>
      </dd>

      <dt>
        Venue
      </dt>
      <dd>
        <% if @venue_like.venue.present? %>
          <a href="/venues/<%= @venue_like.venue_id %>">
            <%= @venue_like.venue.name %>
          </a>
        <% end %>
      </dd>

      <dt>
        Created at
      </dt>
      <dd>
        <%= time_ago_in_words(@venue_like.created_at) %> ago
      </dd>

      <dt>
        Updated at
      </dt>
      <dd>
        <%= time_ago_in_words(@venue_like.updated_at) %> ago
      </dd>
    </dl>
  </div>
</div>


<div class="card">
  <div class="card-header d-flex align-items-center justify-content-between">
    Venue Likes

    <a data-toggle="collapse" href="#user_venue_likes_new_form">
      Add venue like
    </a>
  </div>

  <ul class="list-group list-group-flush">
    <% @user.venue_likes.each do |venue_like| %>
      <li class="list-group-item d-flex align-items-center justify-content-between">
        <a href="/venue_likes/<%= venue_like.id %>">
          <%= venue_like.user_id %>
        </a>

      </li>
    <% end %>

  </ul>
</div>

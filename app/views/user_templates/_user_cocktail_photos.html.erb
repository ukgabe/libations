<div class="card">
  <div class="card-header d-flex align-items-center justify-content-between">
    Cocktail Photos

    <a data-toggle="collapse" href="#user_cocktail_photos_new_form">
      Add cocktail photo
    </a>
  </div>

  <ul class="list-group list-group-flush">
    <% @user.cocktail_photos.each do |cocktail_photo| %>
      <li class="list-group-item d-flex align-items-center justify-content-between">
        <a href="/cocktail_photos/<%= cocktail_photo.id %>">
          <%= cocktail_photo.created_at %>
        </a>

      </li>
    <% end %>

  </ul>
</div>
